{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en_US",
  "useGitignore": true,
  "cache": {
    "useCache": true
  },
  "dictionaries": [
    "softwareTerms",
    "typescript",
    "aws",
    "bash",
    "git",
    "node",
    "npm"
  ],
  "ignorePaths": [
    "node_modules",
    "cspell.json",
    "**/*.js",
    "**/auto_docs/**",
    "**/build/**",
    "**/dist/**",
    "**/coverage/**",
    "**/junit.xml",
    "**/.gitignore"
  ],
  "words": ["commitlint", "conventionalcommits", "Minicraft", "tileset"],
  "flagWords": []
}
