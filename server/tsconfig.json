{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es2015",
    "moduleResolution": "node",
    "pretty": true,
    "newLine": "LF",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "allowJs": true,
    "removeComments": true,
  },
  "exclude": [
    "node_modules",
    "client",
    "preload"
  ]
}